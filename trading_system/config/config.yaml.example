# Configuration file for your trading system
# IMPORTANT: Copy this file to config.yaml and add your real API keys
# DO NOT commit config.yaml to git!

# Exchange Settings
exchange:
  name: "gateio"
  api_key: "YOUR_API_KEY_HERE"
  api_secret: "YOUR_API_SECRET_HERE"

# System Settings
system:
  test_mode: true
  max_api_calls_per_second: 2

# Initial Watchlist
watchlist:
  - "BTC/USDT"
  - "ETH/USDT"
  - "SOL/USDT"
  - "ENA/USDT"
